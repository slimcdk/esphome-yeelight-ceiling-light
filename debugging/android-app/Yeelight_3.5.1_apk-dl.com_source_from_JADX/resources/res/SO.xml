<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:aapt="http://schemas.android.com/aapt">
    <item android:state_pressed="false">
        <shape>
            <corners android:topLeftRadius="@dimen/common_long_btn_radius" android:topRightRadius="0dp" android:bottomLeftRadius="@dimen/common_long_btn_radius" android:bottomRightRadius="0dp"/>
            <solid android:color="@color/common_dialog_btn_solid"/>
            <stroke android:width="1px" android:color="@color/common_dialog_btn_border_color"/>
        </shape>
    </item>
    <item android:state_pressed="true">
        <shape>
            <corners android:topLeftRadius="@dimen/common_long_btn_radius" android:topRightRadius="0dp" android:bottomLeftRadius="@dimen/common_long_btn_radius" android:bottomRightRadius="0dp"/>
            <solid android:color="@color/common_dialog_btn_solid_pressed"/>
            <stroke android:width="1px" android:color="@color/common_dialog_btn_border_color"/>
        </shape>
    </item>
</selector>
