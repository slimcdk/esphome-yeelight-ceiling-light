<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto">
    <item android:state_enabled="true" android:state_pressed="false">
        <shape>
            <corners android:topRightRadius="@dimen/common_long_btn_radius" android:bottomRightRadius="@dimen/common_long_btn_radius"/>
            <solid android:color="@color/common_dialog_btn_solid"/>
            <stroke android:width="1px" android:color="@color/common_dialog_btn_border_color"/>
        </shape>
    </item>
    <item android:state_pressed="true">
        <shape>
            <corners android:topRightRadius="@dimen/common_long_btn_radius" android:bottomRightRadius="@dimen/common_long_btn_radius"/>
            <solid android:color="@color/common_dialog_btn_solid_pressed"/>
            <stroke android:width="1px" android:color="@color/common_dialog_btn_border_color"/>
        </shape>
    </item>
    <item android:state_enabled="false">
        <shape>
            <corners android:topRightRadius="@dimen/common_long_btn_radius" android:bottomRightRadius="@dimen/common_long_btn_radius"/>
            <solid android:color="@color/common_dialog_btn_solid_disable"/>
            <stroke android:width="1px" android:color="@color/common_dialog_btn_border_color"/>
        </shape>
    </item>
</selector>
