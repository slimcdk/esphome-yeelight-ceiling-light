<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto">
    <item android:state_checked="true">
        <shape android:shape="oval" android:useLevel="false">
            <solid android:color="@color/common_color_primary"/>
            <stroke android:width="1px" android:color="@color/common_color_primary"/>
        </shape>
    </item>
    <item android:state_pressed="false">
        <shape android:shape="oval" android:useLevel="false">
            <solid android:color="@color/common_color_secondary_white_f8"/>
            <stroke android:width="1px" android:color="@color/common_color_secondary_white_f8"/>
        </shape>
    </item>
    <item android:state_pressed="true">
        <shape android:shape="oval" android:useLevel="false">
            <solid android:color="@color/common_color_primary"/>
            <stroke android:width="1px" android:color="@color/common_color_primary"/>
        </shape>
    </item>
</selector>
