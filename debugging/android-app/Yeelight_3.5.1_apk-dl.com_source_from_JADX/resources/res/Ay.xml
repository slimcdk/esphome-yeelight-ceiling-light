<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto">
    <item android:state_enabled="false">
        <shape>
            <corners android:radius="55dp"/>
            <solid android:color="@color/gray"/>
            <stroke android:width="1px" android:color="@color/common_color_divider_line"/>
        </shape>
    </item>
    <item android:state_pressed="false">
        <shape>
            <corners android:radius="55dp"/>
            <solid android:color="@color/black_20_transparent"/>
            <stroke android:width="1px" android:color="@color/black_20_transparent"/>
        </shape>
    </item>
    <item android:state_pressed="true">
        <shape>
            <corners android:radius="55dp"/>
            <solid android:color="@color/black_10_transparent"/>
            <stroke android:width="1px" android:color="@color/black_10_transparent"/>
        </shape>
    </item>
</selector>
