<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:aapt="http://schemas.android.com/aapt">
    <item android:state_enabled="false">
        <shape>
            <corners android:radius="55dp"/>
            <solid android:color="@color/common_color_divider_line"/>
            <stroke android:width="1px" android:color="@color/common_color_divider_line"/>
        </shape>
    </item>
    <item android:state_pressed="false">
        <shape>
            <corners android:radius="55dp"/>
            <solid android:color="@color/round_btn_red"/>
            <stroke android:width="1px" android:color="@color/round_btn_red"/>
        </shape>
    </item>
    <item android:state_pressed="true">
        <shape>
            <corners android:radius="55dp"/>
            <solid android:color="@color/round_btn_red_pressed"/>
            <stroke android:width="1px" android:color="@color/round_btn_red_pressed"/>
        </shape>
    </item>
</selector>
