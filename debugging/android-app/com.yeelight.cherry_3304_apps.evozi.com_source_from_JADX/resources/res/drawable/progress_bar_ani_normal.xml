<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:aapt="http://schemas.android.com/aapt">
    <item android:drawable="@drawable/progressbar_indeterminate_bg_light"/>
    <item>
        <rotate android:interpolator="@android:anim/linear_interpolator" android:fromDegrees="0" android:toDegrees="360" android:pivotX="50%" android:pivotY="50%">
            <bitmap android:src="@drawable/progressbar_indeterminate_circle_light" android:antialias="true" android:filter="true"/>
        </rotate>
    </item>
</layer-list>
